//create a class with two public data members of integer type and a member function to input values for data members
#include<iostream>
using namespace std;
 class Private{
    public :

     int a;
     int b;
     public :

     int sum(int a,int b){
        cout<<a+b;
    }
 };

    int main(){
        Private p;
        p.sum(5,6);

        
    }
